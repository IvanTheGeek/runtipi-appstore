{
    "services": [
        {
            "name": "freshrss",
            "image": "lscr.io/linuxserver/freshrss:1.24.3",
            "isMain": true,
            "internalPort": 80,
            "restart": "unless-stopped",
            "environment": {
                "PUID": "1000",
                "PGID": "1000",
                "TZ": "${TZ}"
            },
            "volumes": [
                {
                    "hostPath": "${APP_DATA_DIR}/data/freshrss",
                    "containerPath": "/config"
                }
            ]
        }
    ]
}